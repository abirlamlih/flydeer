          {%- endif -%}
           <span class="sr-only">{{ item.value["#title"] }}</span>{% endset %}
        <li{{ item.attributes }}>{{ link(link_title, item.value["#url"], { 'class' : [] }) }}</li>

       {%- endfor -%}
      </{{ list_type }}>
  {%- else -%}
      {{- empty -}}
  {%- endif -%}
{%- endif %}
